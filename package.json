  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "validator": "^13.12.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "~20.11.29",
    "@types/sequelize": "^4.28.20",
    "@types/sinon-express-mock": "^1.3.12",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "~5.0.5",
    "sinon": "^17.0.1",
    "sinon-express-mock": "^2.2.1",
    "tsc-alias": "^1.8.8",
    "tsc-watch": "^6.0.4",
    "tsutils": "~3.21.0",
    "typescript": "~5.4.2"
  },
  "scripts": {
    "tsc": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start-debug": "node --inspect-brk dist/server.js",
    "clean": "rimraf dist/",
    "compile": "run-s clean tsc start",
    "compile-debug": "run-s clean tsc start-debug",
    "dev": "nodemon",
    "debug": "nodemon --watch src --ext ts --exec 'npm run compile-debug'",
    "build": "run-s clean && echo 'Starting TypeScript compilation...' && tsc && echo 'TypeScript compilation completed.'",
    "lint": "eslint src/",
    "lint:fix": "eslint --cache --fix src/ && prettier --write src/",
    "test": "jest",
    "prepare": "husky install"
  }
}
